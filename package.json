{
  "name": "goin-deep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:ci": "cross-env CI=true jest --silent --passWithNoTests --notify",
    "setup-husky": "husky install",
    "staged-pretty-css": "pretty-quick --staged  --pattern \"**/*.css\" \"**/*.scss\"",
    "staged-pretty-js": "pretty-quick --staged  --pattern \"**/*.js\" \"**/*.jsx\" \"**/*.json\"",
    "test:jest": "jest --notify",
    "precommit": "npx lint-staged",
    "pretty": "prettier --use-tabs --single-quote --write \"**/*.{js,jsx}\"",
    "verify-node-version": "node ./bin/verify-node-version.js"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json}": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "@tanstack/react-query": "^4.3.4",
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "fs": "^0.0.1-security",
    "next": "13.5.4",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.2.0",
    "react-highlight-words": "^0.18.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "react-youtube": "^9.0.3",
    "sass": "^1.54.9"
  },
  "devDependencies": {
    "@jest/types": "^29.0.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "20.8.9",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.2.14",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "node-notifier": "^10.0.1",
    "prettier": "^2.7.1",
    "prettier-cli": "^0.1.0",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "overrides": {
    "react-virtualized-auto-sizer": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
